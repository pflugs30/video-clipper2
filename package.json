{
  "name": "video-clipper",
  "version": "0.1.2",
  "private": true,
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:electron": "tsc --project tsconfig.electron.json && tsc --project tsconfig.preload.json",
    "electron": "electron .",
    "electron:dev": "npm run build:electron && electron .",
    "start": "concurrently \"npm run dev\" \"npm run electron:dev\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "video.js": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  }
}
